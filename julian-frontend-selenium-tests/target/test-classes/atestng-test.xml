<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Adam Frontend Selenium Tests" verbose="5" data-provider-thread-count="1"  parallel="tests" thread-count="6">
  <test name="Export Panel Firefox Ubuntu">
    <parameter name="browserName" value="firefox"/>
    <parameter name="platformName" value="linux"/>
    <parameter name="browserVersion" value=""/>
    <packages>
      <package name="meetrics.pages.export" />
    </packages>
  </test>
  <test name="Export Panel Chrome Windows10">
    <parameter name="browserName" value="chrome"/>
    <parameter name="platformName" value="win10"/>
    <parameter name="browserVersion" value=""/>
    <packages>
      <package name="meetrics.pages.export" />
    </packages>
  </test>
</suite>